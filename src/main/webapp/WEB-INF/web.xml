<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	      http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<display-name>Archetype Created Web Application</display-name>
	<welcome-file-list>
		<welcome-file>/index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>DisplayServ</servlet-name>
		<servlet-class>com.ebix.test.DisplayServ</servlet-class>
		<init-param>
			<param-name>usr1</param-name>
			<param-value>Mani</param-value>
		</init-param>
		<init-param>
			<param-name>usr2</param-name>
			<param-value>Eapp</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisplayServ</servlet-name>
		<url-pattern>/DisplayServ</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>secont</servlet-name>
		<servlet-class>com.ebix.controller.ServController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>secont</servlet-name>
		<url-pattern>/cont.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>red</servlet-name>
		<jsp-file>/Redirect.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>red</servlet-name>
		<url-pattern>/red.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>project</servlet-name>
		<jsp-file>/product.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>project</servlet-name>
		<url-pattern>/project.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/Login.jsp</jsp-file>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ServDao</display-name>
		<servlet-name>ServDao</servlet-name>
		<servlet-class>com.ebix.test.ServDao</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ServController</display-name>
		<servlet-name>ServController</servlet-name>
		<servlet-class>com.ebix.controller.ServController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServDao</servlet-name>
		<url-pattern>/ServDao</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServController</servlet-name>
		<url-pattern>/ServController</url-pattern>
	</servlet-mapping>
	<listener>
		<listener-class>com.ebix.listener.ListenContext</listener-class>
	</listener>
	<context-param>
		<param-name>mail</param-name>
		<param-value>mm@gmail.com</param-value>
	</context-param>
	<filter>
		<description>
		</description>
		<display-name>HibernateSessionRequestFilter</display-name>
		<filter-name>HibernateSessionRequestFilter</filter-name>
		<filter-class>com.ebix.filter.HibernateSessionRequestFilter</filter-class>
	</filter>
	<filter>
		<filter-name>auth</filter-name>
		<filter-class>com.ebix.filter.Authenticate</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>HibernateSessionRequestFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>auth</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<session-config>
		<session-timeout>2</session-timeout>
	</session-config>
</web-app>
