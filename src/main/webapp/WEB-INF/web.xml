<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

	<display-name>Archetype Created Web Application</display-name>
	<welcome-file-list>
		<welcome-file>/index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>red</servlet-name>
		<jsp-file>/jsp/redirect.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>red</servlet-name>
		<url-pattern>/red.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>project</servlet-name>
		<jsp-file>/jsp/product.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>project</servlet-name>
		<url-pattern>/project.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/jsp/login.jsp</jsp-file>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ServDao</display-name>
		<servlet-name>ServDao</servlet-name>
		<servlet-class>com.ebix.test.ServDao</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServDao</servlet-name>
		<url-pattern>/ServDao</url-pattern>
	</servlet-mapping>
	<listener>
		<listener-class>com.ebix.listener.ListenContext</listener-class>
	</listener>
	<context-param>
		<param-name>mail</param-name>
		<param-value>mm@gmail.com</param-value>
	</context-param>
	<filter>
		<description>
		</description>
		<display-name>HibernateSessionRequestFilter</display-name>
		<filter-name>HibernateSessionRequestFilter</filter-name>
		<filter-class>com.ebix.filter.HibernateSessionRequestFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>HibernateSessionRequestFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<session-config>
		<session-timeout>2</session-timeout>
	</session-config>
</web-app>
